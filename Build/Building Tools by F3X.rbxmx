		return;
	end;

	-- Only show and identify current surface type if selection is not empty
	if #Selection.Parts == 0 then
		if self.CurrentSurfaceType ~= '' then
			self.CurrentSurfaceType = ''
			self.OnSurfaceTypeChanged:Fire('')
		end
