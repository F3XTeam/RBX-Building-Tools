			elseif self.Options.mode == "texture" then
				self:addTexture();
			end;
		end );
		Container.RemoveButton.Button.MouseButton1Up:connect( function ()
			if self.Options.mode == "decal" then
				self:removeDecal();
			elseif self.Options.mode == "texture" then
				self:removeTexture();
			end;
		end );

