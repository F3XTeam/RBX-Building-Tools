			if Arg and Minimum and Arg < Minimum then
				Arg = Minimum;
			elseif Arg and Maximum and Arg > Maximum then
				Arg = Maximum;
			end;

			-- Index the filtered argument
			FilteredArgs[Index] = Arg;

		end;

		-- Return the filtered arguments
		return unpack(FilteredArgs);

	end;

