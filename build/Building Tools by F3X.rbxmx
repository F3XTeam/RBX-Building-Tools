			if not HandleDragging or (InputInfo.UserInputType ~= Enum.UserInputType.MouseButton1) then
				return;
			end;

			-- Disable dragging
			HandleDragging = false;

			-- Clear this connection to prevent it from firing again
			Connections.HandleRelease:disconnect();
